local s = game:GetObjects("rbxassetid://84072993261034")[1]
s.Parent = game.Workspace
local currentRoom = workspace.CurrentRooms:GetChildren()[#workspace.CurrentRooms:GetChildren()]
local fixtures = currentRoom:FindFirstChild("Assets", true):FindFirstChild("Light_Fixtures", true)

if fixtures then
    for _, lightStand in pairs(fixtures:GetChildren()) do
        local lightFixture = lightStand:FindFirstChild("LightFixture", true)
        if lightFixture then
            local pointLight = lightFixture:FindFirstChildWhichIsA("PointLight")
            if pointLight then
                pointLight.Enabled = false
            end
        end
    end
end


local ss = s:FindFirstChildWhichIsA("BasePart") or s:FindFirstChildWhichIsA("Part")
ss.CFrame = game.Workspace.CurrentRooms:GetChildren()[#game.Workspace.CurrentRooms:GetChildren() - 1].Parts.Floor.CFrame + Vector3.new(0,15,0)

local ultimaroom = game.ReplicatedStorage.GameData.LatestRoom.Value
local plr = game.Players.LocalPlayer
local chr = plr.Character or plr.CharacterAdded:Wait()
wait(1)
while true do
wait(math.random(0.5, 1))
if not chr:GetAttribute("Crouching") then
game.Players.LocalPlayer.Character.Humanoid.Health -= 3
end
game:GetService("ReplicatedStorage").GameStats["Player_".. game.Players.LocalPlayer.Name].Total.DeathCause.Value = "Silence"
if ultimaroom ~= game.ReplicatedStorage.GameData.LatestRoom.Value then 
s:Destroy()
break end
end
